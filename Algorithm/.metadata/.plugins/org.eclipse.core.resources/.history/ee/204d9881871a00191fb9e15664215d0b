package chapter01;

import java.util.ArrayList;
import java.util.Scanner;

//	chapter01 : 버블소트
public class BubbleSort {
	public static void main(String[] args) {
		int length = 5;	//	배열의 길이
		int temp;
		int data[] = {5,11,7,9,1};
			
		//	bubble sort
		for(int i = 0; i<length; i ++) {	//	시간복잡도 : O(N^2)
			for(int j = 0; j<length-(i+1); j++) {
				if(data[j] > data[j+1]) {	// 양옆의 data 비교
					//	자리교환
					temp = data[j+1];	//	swap
					data[j+1] = data[j];
				}
			}
		}		
	}
}


