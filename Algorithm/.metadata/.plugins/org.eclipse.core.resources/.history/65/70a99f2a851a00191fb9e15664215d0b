package chapter01;

import java.util.ArrayList;
import java.util.Scanner;

//	chapter01 : 버블소트
public class BubbleSort {
	
	
	
	public static void main(String[] args) {
		ArrayList<Integer>[] arr;
		
		Scanner sc = new Scanner(System.in);
		
		for(int i = 0; i<arr.length; i ++) {
			//	배열 입력받기
			arr[i] = sc.nextInt();
			
		}
			
	}
}


