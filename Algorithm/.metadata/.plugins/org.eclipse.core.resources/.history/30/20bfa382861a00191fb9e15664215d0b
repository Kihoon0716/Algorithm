package chapter01;

import java.util.ArrayList;
import java.util.Scanner;

//	chapter01 : 버블소트
public class BubbleSort {
	
	
	
	public static void main(String[] args) {
		int length = 5;
		int temp;
		int data[] = {5,11,7,9,1}
		
		for(int i = 0; i<length; i ++) {
			for(int j = 0; j<length-(i+1); j++) {
				if(data[j] > data[j+1]) {
					temp = data[j+1];
					data[j+1] = data[j];
					data[j] = temp;
				}
			}
		}	
	}
}


