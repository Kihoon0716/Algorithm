package chapter01;

import java.util.Scanner;

//	최빈수구하기
public class Solutin1204 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		//	test case 입력 받기
		int tc = sc.nextInt();
		
		//	학생 수만큼의 점수를 저장하는 배열
		int arr[] = new int[1000];
		
		//	배열 순회하며 1000개의 점수 입력 받기
		for(int i = 0; i<tc; i++) {
			
			for(int j = 0; j<arr.length; j++) {
				arr[i] = sc.nextInt();
			}
			
		}
	}
	
}
